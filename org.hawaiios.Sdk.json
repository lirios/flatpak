            "sources": [
                {
                    "type": "git",
                    "url": "git://anongit.freedesktop.org/gstreamer/gst-plugins-bad"
                }
            ]
        },
        {
            "cleanup-platform": [ "/bin", "/mkspecs" ],
            "config-opts": [
                "-confirm-license",
                "-opensource",
                "-release",
                "-shared",
                "-platform",
                "linux-g++",
                "-optimized-qmake",
                "-verbose",
                "-nomake",
                "examples",
                "-system-harfbuzz",
                "-system-sqlite",
                "-no-alsa",
                "-accessibility",
                "-dbus-linked",
                "-fontconfig",
                "-glib",
                "-iconv",
                "-icu",
                "-openssl-linked",
                "-no-pch",
                "-no-rpath",
                "-no-separate-debug-info",
                "-reduce-relocations",
                "-no-directfb",
                "-no-linuxfb",
                "-no-eglfs",
                "-no-kms",
                "-no-gbm",
                "-no-cups",
                "-no-mirclient",
                "-system-proxies"
