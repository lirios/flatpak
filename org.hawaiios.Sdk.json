{
    "branch": "master",
    "build-options": {
        "arch": {
            "x86_64": {
                "cflags": "-O3 -g",
                "cxxflags": "-O3 -g"
            }
        },
        "cflags": "-O2 -g",
        "cxxflags": "-O2 -g",
        "env": {
            "V": "1"
        }
    },
    "build-runtime": true,
    "cleanup": [
        "/cache",
        "/man",
        "/share/doc",
        "/share/man",
        "/share/gtk-doc",
        "/share/devhelp",
        "/lib/systemd",
        "*.la",
        "*.a"
    ],
    "cleanup-commands": [
        "touch -d @0 /usr/share/fonts",
        "touch -d @0 /usr/share/fonts/*",
        "fc-cache -fs"
    ],
    "cleanup-platform": [
        "/include",
        "/share/aclocal",
        "/share/pkgconfig",
        "/lib/pkgconfig",
        "/share/gir-1.0",
        "/share/vala"
    ],
    "id": "org.hawaiios.Sdk",
    "id-platform": "org.hawaiios.Platform",
    "metadata": "metadata.sdk",
    "metadata-platform": "metadata.platform",
    "modules": [
        {
            "config-opts": [
                "--with-pcre=system"
            ],
            "name": "glib",
            "sources": [
                {
                    "type": "git",
                    "url": "git://git.gnome.org/glib"
                }
            ]
        },
        {
            "config-opts": [
                "--disable-static"
            ],
            "name": "gobject-introspection",
            "sources": [
                {
                    "type": "git",
                    "url": "git://git.gnome.org/gobject-introspection"
                }
            ]
        },
        {
            "name": "pango",
            "sources": [
                {
                    "type": "git",
                    "url": "git://git.gnome.org/pango"
                }
            ]
        },
        {
            "name": "atk",
            "sources": [
                {
                    "type": "git",
                    "url": "git://git.gnome.org/atk"
                }
            ]
        },
        {
            "name": "at-spi2-core",
            "sources": [
                {
                    "type": "git",
                    "url": "git://git.gnome.org/at-spi2-core"
                }
            ]
        },
        {
            "name": "at-spi2-atk",
            "sources": [
                {
                    "type": "git",
                    "url": "git://git.gnome.org/at-spi2-atk"
                }
            ]
        },
        {
            "config-opts": [
                "--disable-gtk-doc",
                "--enable-debug",
                "--disable-examples"
            ],
            "name": "gstreamer",
            "sources": [
                {
                    "type": "git",
                    "url": "git://anongit.freedesktop.org/gstreamer/gstreamer"
                }
            ]
        },
        {
            "config-opts": [
                "--disable-gtk-doc",
                "--enable-experimental",
                "--enable-orc"
            ],
            "name": "gstreamer-plugins-base",
            "sources": [
                {
                    "type": "git",
                    "url": "git://anongit.freedesktop.org/gstreamer/gst-plugins-base"
                }
            ]
        },
        {
            "config-opts": [
                "--disable-gtk-doc",
                "--enable-experimental",
                "--enable-orc",
                "--disable-monoscope",
                "--disable-aalib",
                "--disable-cairo",
                "--disable-libcaca",
                "--disable-jack",
                "--with-default-visualizer=autoaudiosink"
            ],
            "name": "gstreamer-plugins-good",
            "sources": [
                {
                    "type": "git",
                    "url": "git://anongit.freedesktop.org/gstreamer/gst-plugins-good"
                }
